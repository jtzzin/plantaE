{
  "expo": {
    "name": "PlantaE",
    "slug": "plantae",
    "orientation": "portrait",
    "scheme": "plantae",
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#3BB39D",
          "sounds": [
            "alarm.wav"
          ]
        }
      ],
      "expo-font",
      "expo-router"
    ],
    "notification": {
      "iosDisplayInForeground": true
    },
    "android": {
      "package": "com.exemplo.plantae",
      "permissions": [
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "useNextNotificationsApi": true
    },
    "ios": {
      "bundleIdentifier": "com.exemplo.plantae"
    }
  }
}
